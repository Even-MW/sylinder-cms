{
  "root": true,
  "project": {
    "name": "SylinderCMS"
  },
  "api": {
    "projectId": "clrua9n3",
    "dataset": "unil"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/production-preview",
    "iframe-pane",
    "seo-pane",
    "media",
    "asset-source-unsplash",
    "content-calendar",
    "@sanity/dashboard",
    "dashboard-widget-document-list"
  ],
  "env": {
